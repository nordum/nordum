{
  "name": "nordum",
  "version": "0.9.0+1758124142536",
  "description": "A pan-Scandinavian written language designed for maximum mutual intelligibility across Norwegian, Danish, and Swedish",
  "main": "build/index.html",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "live-server build --port=3000 --open",
    "dev:build": "npm run build && npm run watch",
    "build": "npm-run-all build:*",
    "build:clean": "rimraf build",
    "build:specification": "node scripts/parse-specification.js",
    "build:templates": "node scripts/build-templates.js",
    "build:styles": "sass src/styles/main.scss build/assets/css/main.css --style=compressed",
    "build:scripts": "webpack --mode=production --config webpack.config.js",
    "build:dictionary": "node scripts/build-dictionary.js",
    "build:assets": "cpx 'src/assets/**/*' build/assets",
    "build:i18n": "node scripts/build-i18n.js",
    "import:all": "node scripts/import-dictionaries.js",
    "import:frequency": "node scripts/importers/frequency-importer.js",
    "import:norwegian": "node scripts/importers/norwegian-importer.js",
    "import:danish": "node scripts/importers/danish-importer.js",
    "import:swedish": "node scripts/importers/swedish-importer.js",
    "cache:setup": "node scripts/importers/setup-cache.js",
    "cache:warm": "node scripts/importers/manage-cache.js warm-quick",
    "cache:warm-full": "node scripts/importers/manage-cache.js warm-full",
    "cache:stats": "node scripts/importers/manage-cache.js stats",
    "cache:analyze": "node scripts/importers/manage-cache.js analyze",
    "cache:clean": "node scripts/importers/manage-cache.js clean",
    "cache:clear": "node scripts/importers/manage-cache.js clear",
    "cache:estimate": "node scripts/importers/manage-cache.js estimate",
    "test:apis": "node scripts/importers/test-apis.js quick",
    "test:apis-full": "node scripts/importers/test-apis.js full",
    "test:cache": "node scripts/importers/test-cache.js quick",
    "test:cache-full": "node scripts/importers/test-cache.js",
    "version": "node scripts/version-manager.js current",
    "version:major": "node scripts/version-manager.js major",
    "version:minor": "node scripts/version-manager.js minor",
    "version:patch": "node scripts/version-manager.js patch",
    "version:build": "node scripts/version-manager.js build",
    "version:prerelease": "node scripts/version-manager.js prerelease",
    "version:stable": "node scripts/version-manager.js stable",
    "version:history": "node scripts/version-manager.js list",
    "watch": "npm-run-all --parallel watch:*",
    "watch:specification": "chokidar 'NORDUM_LANGUAGE_SPECIFICATION.md' -c 'npm run build:specification && npm run build:templates'",
    "watch:templates": "chokidar 'src/**/*.hbs' 'src/**/*.json' -c 'npm run build:templates'",
    "watch:styles": "sass --watch src/styles/main.scss:build/assets/css/main.css",
    "watch:scripts": "webpack --mode=development --watch --config webpack.config.js",
    "watch:dictionary": "chokidar 'data/dictionary/**/*' -c 'npm run build:dictionary'",
    "watch:i18n": "chokidar 'src/i18n/**/*' -c 'npm run build:i18n'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "validate:dictionary": "node scripts/validate-dictionary.js",
    "serve": "live-server build --port=3000",
    "deploy": "npm run build && gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordum/nordum.git"
  },
  "keywords": [
    "nordum",
    "scandinavian",
    "language",
    "norwegian",
    "danish",
    "swedish",
    "linguistics",
    "pan-scandinavian",
    "constructed-language"
  ],
  "author": "Nordum Project Contributors",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/nordum/nordum/issues"
  },
  "homepage": "https://nordum.org",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "chokidar-cli": "^3.0.0",
    "cpx": "^1.2.1",
    "eslint": "^8.52.0",
    "gh-pages": "^6.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.2",
    "natural": "^6.10.0",
    "papaparse": "^5.4.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
